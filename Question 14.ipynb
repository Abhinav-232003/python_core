   ],
   "source": [
    "b = \"Helloworld\"\n",
    "c=0\n",
    "d=0\n",
    "for i in b:\n",
    "    if i.isupper():\n",
    "        c=c+1\n",
    "    else:\n",
    "        d=d+1\n",
    "print(c,d)"
   ]
